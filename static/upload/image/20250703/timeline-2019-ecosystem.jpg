<svg width="600" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGradient5" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="buildingGradient5" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#4682B4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2F4F4F;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="cloudGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0E0E0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="tradingGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#32CD32;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#228B22;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="600" height="300" fill="url(#skyGradient5)"/>
  
  <!-- Ground -->
  <rect x="0" y="250" width="600" height="50" fill="#228B22"/>
  
  <!-- Main facility (expanded) -->
  <rect x="30" y="120" width="200" height="130" fill="url(#buildingGradient5)" stroke="#000" stroke-width="2"/>
  
  <!-- Additional buildings -->
  <rect x="250" y="140" width="120" height="110" fill="url(#buildingGradient5)" stroke="#000" stroke-width="2"/>
  <rect x="390" y="160" width="100" height="90" fill="url(#buildingGradient5)" stroke="#000" stroke-width="2"/>
  
  <!-- Cloud mining representation -->
  <ellipse cx="150" cy="80" rx="40" ry="25" fill="url(#cloudGradient)" stroke="#000" stroke-width="2"/>
  <ellipse cx="130" cy="70" rx="30" ry="20" fill="url(#cloudGradient)" stroke="#000" stroke-width="2"/>
  <ellipse cx="170" cy="70" rx="30" ry="20" fill="url(#cloudGradient)" stroke="#000" stroke-width="2"/>
  
  <!-- Cloud mining connections -->
  <line x1="150" y1="105" x2="150" y2="120" stroke="#0000FF" stroke-width="3" stroke-dasharray="5,5"/>
  <line x1="130" y1="90" x2="130" y2="120" stroke="#0000FF" stroke-width="3" stroke-dasharray="5,5"/>
  <line x1="170" y1="90" x2="170" y2="120" stroke="#0000FF" stroke-width="3" stroke-dasharray="5,5"/>
  
  <!-- Trading platform screen -->
  <rect x="420" y="100" width="120" height="80" fill="#000" stroke="#00FF00" stroke-width="3"/>
  
  <!-- Trading chart -->
  <polyline points="430,150 440,140 450,145 460,130 470,135 480,120 490,125 500,110 510,115 520,105 530,110" 
            stroke="#00FF00" stroke-width="3" fill="none"/>
  
  <!-- Trading indicators -->
  <text x="480" y="175" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#00FF00">BTC/USD</text>
  
  <!-- Ecosystem connections -->
  <line x1="230" y1="180" x2="250" y2="180" stroke="#FFD700" stroke-width="4"/>
  <line x1="370" y1="200" x2="390" y2="200" stroke="#FFD700" stroke-width="4"/>
  
  <!-- Service icons -->
  <circle cx="80" cy="200" r="15" fill="#32CD32" stroke="#000" stroke-width="2"/>
  <text x="80" y="207" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#FFF">M</text>
  
  <circle cx="310" cy="200" r="15" fill="#FF6347" stroke="#000" stroke-width="2"/>
  <text x="310" y="207" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#FFF">T</text>
  
  <circle cx="440" cy="220" r="15" fill="#4169E1" stroke="#000" stroke-width="2"/>
  <text x="440" y="227" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#FFF">C</text>
  
  <!-- Service labels -->
  <text x="80" y="235" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#000">Mining</text>
  <text x="310" y="235" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#000">Trading</text>
  <text x="440" y="245" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#000">Cloud</text>
  
  <!-- Global network representation -->
  <circle cx="500" cy="50" r="8" fill="#FFD700"/>
  <circle cx="520" cy="70" r="6" fill="#FFD700"/>
  <circle cx="480" cy="70" r="6" fill="#FFD700"/>
  
  <line x1="500" y1="50" x2="520" y2="70" stroke="#FFD700" stroke-width="2"/>
  <line x1="500" y1="50" x2="480" y2="70" stroke="#FFD700" stroke-width="2"/>
  
  <!-- Title text -->
  <text x="300" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#000">2019-Present: Ecosystem Development</text>
</svg>
